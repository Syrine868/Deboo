                                                                                                        <a href="{{ path('profil_User') }}"> <i class="fa fa-user"></i>   Profile</a>
                                                                                                </li>

                                                                                                <li><a href="{{ path('fos_user_change_password') }}"><i class="fa fa-lock"></i>    Mot de passe</a></li>
                                                                                            {% if is_granted('ROLE_USER') %}
                                                                                                <li><a class="logout" href="{{ path('fos_user_security_logout') }}"><i class="fa fa-power-off"> Déconnexion</i></a></li>
                                                                                            {% else %}
                                                                                                <li><a href="{{ path('fos_user_security_login') }}">Login</a></li>
                                                                                            {% endif %}
                                                                                        </ul>
                                                                                </li>

                                                                        </ul>
                                                                </nav>
                                                        </div>
                                                </div>
                                                <!-- Mobile Menu -->
