    <br><br>


        <div class="slider-area ">
                <div class="single-slider slider-height slider-padding f-right " >
                        <div class="container">
                                <div class="col-lg-12">

                                                <div id="banner-12345">
                                                    Notre site Web est rendu possible en affichant des publicités en ligne à nos visiteurs. <br>
                                                          Veuillez nous soutenir en désactivant votre bloqueur de publicités sur notre site Web.
                                                </div>
                                                <style>
                                                        #banner-12345 {
                                                                display: none;
                                                                margin-bottom: 70px;
                                                                padding: 50px 40px;
                                                                border-radius: 5px;
                                                                background: #D30000;
                                                                color: #fff;
                                                                text-align: center;
                                                                font-weight: bold;
                                                                width: 350px;
                                                                height: 200px;

                                                        }
                                                </style>
                                                {% set ads = ads() %}
                                                {% for ad in ads %}
                                                <div class="adBanner ">
                                                                <br><br><br><br><br>
                                                        <img src="{{ vich_uploader_asset(ad, 'imageFile') }}"  id="ad">

                                                        <script>
                                                                if($("#ad").height() > 0)
                                                                {

                                                                        document.getElementById('ad').style.height="200";
                                                                        document.getElementById('ad').style.width="350";
                                                                }
                                                                else
                                                                {

                                                                        document.getElementById('banner-12345').style.display='block';


                                                                }

                                                                let ad = document.getElementById('ad');
                                                                ad.addEventListener('click', function(event) {fetch("{{ path('ad_click', {id: ad.id}) }}");});


                                                        </script>
                                                        {% endfor %}
                                                </div>

                                </div>
                        </div>
                </div>
        </div>



