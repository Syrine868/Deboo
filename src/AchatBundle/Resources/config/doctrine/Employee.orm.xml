<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatBundle\Entity\Employee" table="employee">
    <id name="idemp" type="integer" column="idEmp">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastnameemp" type="string" column="lastNameEmp" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstnameemp" type="string" column="firstNameEmp" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="age" type="integer" column="age" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="phone" type="integer" column="phone" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="salary" type="float" column="salary" precision="10" scale="0" nullable="false"/>
    <field name="emailaddress" type="string" column="emailAddress" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fonction" type="string" column="fonction" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
