<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatBundle\Entity\Opinion" table="opinion">
    <indexes>
      <index name="id" columns="id"/>
    </indexes>
    <id name="idopinion" type="integer" column="idOpinion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="opinion" type="integer" column="opinion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rating" type="integer" column="rating" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="id" type="integer" column="id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
